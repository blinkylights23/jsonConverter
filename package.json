{
  "name": "jsonconverter",
  "version": "0.0.0",
  "description": "JSON-to-JSON converter for providing transformations in hand-rolled ETL data pipelines",
  "main": "dist/index.js",
  "files": [
    "dist/*",
    "schema/*",
    "README*",
    "LICENSE/*"
  ],
  "scripts": {
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watchAll",
    "build": "webpack -r esm --mode production --config webpack/webpack.config.js",
    "build:watch": "webpack -w -r esm --mode development --config webpack/webpack.config.js",
    "watch": "concurrently \"npm run *:watch\""
  },
  "keywords": [
    "json2json",
    "transformation",
    "mapping"
  ],
  "author": "Paul Smith <paul@paulsmith.io>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "JSONStream": "^1.3.5",
    "apisauce": "^1.1.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "eslint": "^6.1.0",
    "eslint-utils": "^1.4.2",
    "esm": "^3.2.25",
    "highland": "^2.13.5",
    "jest": "^24.8.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "jmespath": "^0.15.0"
  }
}
