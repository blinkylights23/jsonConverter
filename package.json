{
  "name": "@paulsmith/jsonconverter",
  "version": "0.0.1-2",
  "description": "JSON-to-JSON converter",
  "main": "dist/index.js",
  "files": [
    "dist/*",
    "schema/*",
    "README*",
    "LICENSE/*"
  ],
  "scripts": {
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watchAll",
    "build": "webpack -r esm --mode production --config webpack/webpack.config.js",
    "build:watch": "webpack -w -r esm --mode development --config webpack/webpack.config.js",
    "watch": "concurrently \"npm run *:watch\"",
    "release": "np"
  },
  "keywords": [
    "json2json",
    "transformation",
    "mapping"
  ],
  "author": "Paul Smith <paul@paulsmith.io>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "JSONStream": "^1.3.5",
    "apisauce": "^1.1.1",
    "babel-loader": "^8.1.0",
    "concurrently": "^4.1.2",
    "eslint": "^6.8.0",
    "eslint-utils": "^1.4.3",
    "esm": "^3.2.25",
    "highland": "^2.13.5",
    "jest": "^24.9.0",
    "np": "^6.2.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "jmespath": "^0.15.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
